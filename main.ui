/* 전체 위젯 배경 색상 및 배경 이미지 */
QCalendarWidget {
    background-color: #2e2e2e;
    border: 1px solid #444;
    border-radius: 5px;
    background-position: center;
    background-repeat: no-repeat;
}

/* 헤더(월/연도 선택 영역) 스타일링 */
QCalendarWidget QToolButton {
    border: none; /* 버튼의 테두리 제거 */
    background-color: transparent; /* 배경색 투명하게 설정 */
    color: white;
    font-size: 16px;
    height: 30px;
    border: none;
    border-radius: 5px;
	font: 600 10pt &quot;Segoe UI Variable Small Semibol&quot;;
}

QCalendarWidget QToolButton::hover {
    background-color: rgba(102, 102, 102, 0.8); /* 투명도 적용 */
}

/* 헤더의 화살표 버튼 스타일링 */
QCalendarWidget QToolButton#qt_calendar_prevmonth {
	
	
	background-position: center; /* 이미지가 가운데 오도록 설정 */
    background-repeat: no-repeat;
    width: 30px; /* 버튼의 너비 지정 */
    height: 30px; /* 버튼의 높이 지정 */
    border: none; /* 버튼의 테두리 제거 */
    background-color: transparent; /* 배경색 투명하게 설정 */
}

QCalendarWidget QToolButton#qt_calendar_nextmonth {
	
	background-position: center; /* 이미지가 가운데 오도록 설정 */
    background-repeat: no-repeat;
    width: 30px; /* 버튼의 너비 지정 */
    height: 30px; /* 버튼의 높이 지정 */
    border: none; /* 버튼의 테두리 제거 */
    background-color: transparent; /* 배경색 투명하게 설정 */
}

/* 날짜 셀 스타일링 */
QCalendarWidget QAbstractItemView {

    background-color: rgba(58, 58, 58, 0.8); /* 투명도 적용 */
    selection-background-color: #4CAF50;
    selection-color: white;
    border: none;
    outline: 0;
}

QCalendarWidget QAbstractItemView::item {
    height: 30px;
    width: 30px;
    margin: 3px;
    color: white;
    background-color: rgba(58, 58, 58, 0.6); /* 셀 배경에 투명도 적용 */
}

QCalendarWidget QAbstractItemView::item:selected {
    background-color: #4CAF50;
    color: white;
    border-radius: 15px;
}

/* 오늘 날짜 스타일링 */
QCalendarWidget QWidget#qt_calendar_calendarview .QWidget {
    color: white;
    background-color: #FF5722;
    border-radius: 15px;
}

/* 요일 헤더 스타일링 */
QCalendarWidget QTableView QHeaderView::section {
    background-color: rgba(85, 85, 85, 0.8); /* 투명도 적용 */
    color: #888; /* 요일 글자 색상을 어두운 회색으로 변경 */
    font-size: 14px;
    border: none;
    height: 30px;
}

/* 현재 월에 속하지 않는 날짜 셀 스타일링 */
QCalendarWidget QAbstractItemView::item:disabled {
    background-color: rgba(30, 30, 30, 0.6); /* 현재 월에 속하지 않는 날짜의 배경 색상 */
    color: rgba(255, 255, 255, 0.5); /* 현재 월에 속하지 않는 날짜의 글자 색상 */
}
